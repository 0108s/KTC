{
  "manifest_version": 3,
  "name": "KeyTapCat",
  "version": "2.0.0",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "icons": {
    "16": "cat.png",
    "48": "cat.png",
    "128": "cat.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["cat.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
